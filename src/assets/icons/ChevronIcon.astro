---
interface Props {
	class?: string
}

const { class: className = '' } = Astro.props
---

<svg
	width="100%"
	viewBox="0 0 10 6"
	fill="none"
	xmlns="http://www.w3.org/2000/svg"
	class={className}
>
	<path
		d="M1 1L5 5L9 1"
		strokeOpacity="0.8"
		strokeWidth="1.33333"
		strokeLinecap="round"
		strokeLinejoin="round"></path>
</svg>
